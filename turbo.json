{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "APP_ROUTER_URL",
    "PAGES_ROUTER_URL",
    "APP_PAGES_ROUTER_URL"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "e2e:test": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}