    "next": "^14.2.35 || ~15.0.7 || ~15.1.11 || ~15.2.8 || ~15.3.8 || ~15.4.10 || ~15.5.9 || ^16.0.10"
  },
  "bugs": {
    "url": "https://github.com/opennextjs/opennextjs-aws/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opennextjs/opennextjs-aws",
    "directory": "packages/open-next"
